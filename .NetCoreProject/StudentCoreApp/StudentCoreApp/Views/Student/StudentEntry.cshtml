@model StudentCoreApp.Models.Student
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<link rel="stylesheet" href="~/Content/StyleSheet.css" type="text/css" />
<style>
   div{
       background-color:cornflowerblue;
       font-family:'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
       color:yellow;
       
       justify-content:center;
   }
</style>
<hr />
<a class="btn btn-primary" asp-action="Index">Index</a>
<hr />
<form method="post">
    <div>
        <table>
            <tr>
                <td>
                    Roll
                </td>
                <td>
                    @Html.TextBoxFor(obj => obj.Roll)
                </td>
            </tr>
            <tr>
                <td>
                    Name
                </td>
                <td>
                    @Html.TextBoxFor(obj => obj.Name)
                </td>
            </tr>
            <tr>
                <td>
                    Email
                </td>
                <td>
                    @Html.TextBoxFor(obj => obj.Email)
                </td>
            </tr>
            <tr>
                <td>
                    Password
                </td>
                <td>
                    @Html.TextBoxFor(obj => obj.Psw)
                </td>
            </tr>
        </table>
        <input type="submit" class="btn btn-primary" value="Save" />

    </div>
    @ViewBag.message
</form>